{
  "name": "mbrcld",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "chrome": "open -n -a /Applications/Google\\ Chrome.app --args --user-data-dir=\"/Users/Shared/chrome_without_security_files\" --disable-web-security",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=en --port=4200 --serve-path=/ --base-href=/",
    "start:en": "ng serve --configuration=en --port=4200 --serve-path=/ --base-href=/en/",
    "start:ar": "ng serve --configuration=ar --port=4201 --serve-path=/ --base-href=/ar/",
    "winStart": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng serve --configuration=en --port=4200 --serve-path=/ --base-href=/",
    "winStart:en": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng serve --configuration=en --port=4200 --serve-path=/ --base-href=/en/",
    "winStart:ar": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng serve --configuration=ar --port=4201 --serve-path=/ --base-href=/ar/",
    "build": "ng build",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration=production,en",
    "build:prod:multilang": "cross-env NODE_ENV=production ng build --prod --localize",
    "build:ssr": "cross-env NODE_ENV=production ng run MBRCLD:server:dev",
    "test": "ng test",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts,.json --fix",
    "e2e": "ng e2e",
    "i18n": "ng xi18n --ivy --output-path ./src/locale/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.3",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "10.1.3",
    "@angular/compiler": "10.1.3",
    "@angular/core": "10.1.3",
    "@angular/forms": "10.1.3",
    "@angular/localize": "^10.1.3",
    "@angular/platform-browser": "10.1.3",
    "@angular/platform-browser-dynamic": "10.1.3",
    "@angular/platform-server": "10.1.3",
    "@angular/router": "10.1.3",
    "@aspnet/signalr": "^1.1.4",
    "@fullcalendar/angular": "^5.5.0",
    "@fullcalendar/core": "^5.5.0",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/list": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.0",
    "@iplab/ngx-file-upload": "^2.0.9",
    "@microsoft/signalr": "^5.0.3",
    "@microsoft/signalr-protocol-msgpack": "^5.0.3",
    "angular-oauth2-oidc": "^10.0.3",
    "bootstrap": "^4.3.1",
    "core-js": "^3.3.3",
    "file-saver": "^2.0.5",
    "jquery": "^3.5.1",
    "libphonenumber-js": "^1.7.57",
    "moment": "^2.30.0",
    "oidc-client": "^1.10.1",
    "popper.js": "^1.16.0",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.0",
    "quill": "^1.3.7",
    "rxjs": "^6.5.3",
    "secure-ls": "^1.2.6",
    "slim-select": "^3.4.1",
    "swiper": "^11.1.14",
    "tslib": "^2.0.0",
    "tus-js-client": "^2.2.0",
    "vanillajs-datepicker": "^1.3.4",
    "world-flags-sprite": "0.0.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.0",
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "^10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@angular/language-service": "^10.1.3",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@types/vanillajs-datepicker": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.2.11",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "tailwindcss": "^1.6.1",
    "tailwindcss-rtl": "^0.4.1",
    "typescript": "3.9.7"
  },
  "optionalDependencies": {
    "node-sass": "^4.12.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": "eslint --fix"
  }
}
